<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
 
<web-app>
  <display-name>Archetype Created Web Application</display-name>
 
    <servlet>
        <servlet-name>GuestServlet</servlet-name>
        <servlet-class>com.servlet.GuestServlet</servlet-class>
</servlet>

    <servlet>
         <servlet-name>AllGuests</servlet-name>
        <servlet-class>com.servlet.GuestSortingServlet</servlet-class>
    </servlet>

 
   <servlet-mapping>
        <servlet-name>GuestServlet</servlet-name>
        <url-pattern>/guest</url-pattern>
     </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllGuests</servlet-name>
        <url-pattern>/allguests</url-pattern>

    </servlet-mapping>
 
</web-app>